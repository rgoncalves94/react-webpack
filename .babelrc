{
    "presets": [
        [
            "@babel/env",
            {
                "useBuiltIns": "entry",
                "targets": {
                    "browsers": [
                        "last 2 versions"
                    ]
                }
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/syntax-dynamic-import"
    ]
}